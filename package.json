{
  "name": "disneyunitedway-mern",
  "version": "1.0.0",
  "description": "Disney United Way Cast Donation Portal",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Richard G. Scates",
  "license": "ISC",
  "dependencies": {
    "@pusher/chatkit": "^0.7.18",
    "@pusher/chatkit-client": "^1.13.2",
    "@pusher/chatkit-server": "^2.1.1",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "googleapis": "^39.2.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mongoose": "^5.6.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-bootstrap-dropdown-menu": "^1.1.15",
    "react-items-carousel": "^2.8.0",
    "pusher-chatkit-client": "^0.6.2",
    "pusher-chatkit-server": "^0.12.0",
    "readline": "^1.3.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
